<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="2dp"
    android:layout_marginLeft="2dp"
    android:layout_marginRight="2dp"
    android:backgroundTint="#FFFFFF"
    android:outlineAmbientShadowColor="@color/colorPrimaryDark"
    android:outlineSpotShadowColor="@color/colorPrimaryDark"
    app:cardElevation="2dp"
    app:cardUseCompatPadding="true">
    <!--    app:cardUseCompatPadding="true"-->
<!--    android:backgroundTint="@color/white"-->



    <LinearLayout
        android:id="@+id/llServiceList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:background="@android:color/transparent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:paddingLeft="4dp"
                    android:fontFamily="@font/myfont"
                    android:text="Ticket No"
                    android:textColor="@color/greydarks"
                    android:layout_centerVertical="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:padding="2dp"
                        android:src="@drawable/svg_invoce_no"
                        app:tint="@color/colorPrimary" />

                    <TextView
                        android:id="@+id/tv_TicketNo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="14dp"
                        android:paddingLeft="2dp"
                        android:fontFamily="@font/myfont"
                        android:text="GGG"
                        android:textStyle="normal"
                        android:textColor="@color/black"
                        android:layout_centerVertical="true" />


                </LinearLayout>



                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="12dp"
                        android:paddingLeft="4dp"
                        android:fontFamily="@font/myfont"
                        android:text="Ticket Date"
                        android:textColor="@color/greydarks"
                        android:layout_centerVertical="true" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:padding="2dp"
                            android:src="@drawable/svg_invoce_date"
                            app:tint="@color/colorPrimary" />


                        <TextView
                            android:id="@+id/tv_TicketDate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:textSize="14dp"
                            android:paddingLeft="2dp"
                            android:fontFamily="@font/myfont"
                            android:text="hffh"
                            android:textStyle="normal"
                            android:textColor="@color/black"
                            android:layout_centerVertical="true" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/im_edit"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/ic_edit"
                        android:layout_alignParentLeft="true"
                        android:tint="@color/greydark"
                        android:visibility="visible"
                        tools:ignore="UseAppTint" />

                </LinearLayout>






                <View
                    android:layout_width="0.5dp"
                    android:layout_height="1dp"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:paddingLeft="4dp"
                    android:fontFamily="@font/myfont"
                    android:text="Branch"
                    android:textColor="@color/greydarks"
                    android:layout_centerVertical="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:padding="2dp"
                        android:src="@drawable/svg_invoce_no"
                        app:tint="@color/colorPrimary" />

                    <TextView
                        android:id="@+id/tv_Branch"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="14dp"
                        android:paddingLeft="2dp"
                        android:fontFamily="@font/myfont"
                        android:text="hfghfghfghfg"
                        android:textStyle="normal"
                        android:textColor="@color/black"
                        android:layout_centerVertical="true" />


                </LinearLayout>



                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:paddingLeft="4dp"
                    android:fontFamily="@font/myfont"
                    android:text="Customer"
                    android:textColor="@color/greydarks"
                    android:layout_centerVertical="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:padding="2dp"
                        android:src="@drawable/svg_invoce_date"
                        app:tint="@color/colorPrimary" />


                    <TextView
                        android:id="@+id/tv_Customer"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="14dp"
                        android:paddingLeft="2dp"
                        android:fontFamily="@font/myfont"
                        android:text="kyuytrututr"
                        android:textStyle="normal"
                        android:textColor="@color/black"
                        android:layout_centerVertical="true" />

                </LinearLayout>



                <View
                    android:layout_width="0.5dp"
                    android:layout_height="1dp"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:paddingLeft="4dp"
                    android:fontFamily="@font/myfont"
                    android:text="Mobile"
                    android:textColor="@color/greydarks"
                    android:layout_centerVertical="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:padding="2dp"
                        android:src="@drawable/icon_svg_attendedby"
                        app:tint="@color/colorPrimary" />

                    <TextView
                        android:id="@+id/tv_Mobile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="14dp"
                        android:paddingLeft="2dp"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textStyle="normal"
                        android:textColor="@color/black"
                        android:layout_centerVertical="true" />


                </LinearLayout>



                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:paddingLeft="4dp"
                    android:fontFamily="@font/myfont"
                    android:text="Area"
                    android:textColor="@color/greydarks"
                    android:layout_centerVertical="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:padding="2dp"
                        android:src="@drawable/svg_invoce_date"
                        app:tint="@color/colorPrimary" />


                    <TextView
                        android:id="@+id/tv_Area"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="14dp"
                        android:paddingLeft="2dp"
                        android:fontFamily="@font/myfont"
                        android:text="fdhdfhdfh"
                        android:textStyle="normal"
                        android:textColor="@color/black"
                        android:layout_centerVertical="true" />

                </LinearLayout>



                <View
                    android:layout_width="0.5dp"
                    android:layout_height="1dp"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_employee"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="12dp"
                    android:paddingLeft="4dp"
                    android:fontFamily="@font/myfont"
                    android:text="Employee"
                    android:textColor="@color/greydarks"
                    android:layout_centerVertical="true" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:padding="2dp"
                        android:src="@drawable/svg_invoce_no"
                        app:tint="@color/colorPrimary" />

                    <TextView
                        android:id="@+id/tv_Employee"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textSize="14dp"
                        android:paddingLeft="2dp"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textStyle="normal"
                        android:textColor="@color/black"
                        android:layout_centerVertical="true" />


                </LinearLayout>



                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/color_background1"
                    android:visibility="gone"/>


            </LinearLayout>




        </LinearLayout>



        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:background="#ECECEA"
            android:visibility="gone"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_service_list"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="12dp"
                android:paddingLeft="4dp"
                android:fontFamily="@font/myfont"
                android:text="Channel"
                android:textColor="@color/greydark"
                android:layout_centerVertical="true" />

            <ImageView
                android:id="@+id/im_Channel"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:visibility="visible" />

            <!--            TicketNo": "TKT-0032-->
<!--            app:tint="@color/red"-->

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="12dp"
                android:paddingLeft="4dp"
                android:fontFamily="@font/myfont"
                android:text="Priority"
                android:textColor="@color/greydark"
                android:layout_centerVertical="true" />

            <ImageView
                android:id="@+id/im_Priority"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:visibility="visible" />

<!--            app:tint="@color/red"-->
<!--            android:src="@drawable/dot_regular_svg"-->

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_status"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="12dp"
                android:paddingLeft="4dp"
                android:fontFamily="@font/myfont"
                android:text="Status"
                android:textColor="@color/greydark"
                android:layout_centerVertical="true" />

            <ImageView
                android:id="@+id/im_Status"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:visibility="visible" />
<!--            android:src="@drawable/dot_svg"-->
<!--            app:tint="@color/red"-->

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="12dp"
                android:paddingLeft="4dp"
                android:fontFamily="@font/myfont"
                android:text="Time Due"
                android:textColor="@color/greydark"
                android:layout_centerVertical="true" />

            <TextView
                android:id="@+id/tv_TimeDue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="14dp"
                android:paddingLeft="2dp"
                android:fontFamily="@font/myfont"
                android:text="5 Months ago"
                android:textStyle="normal"
                android:textColor="@color/black"
                android:layout_centerVertical="true" />

        </LinearLayout>

    </LinearLayout>






    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/list_line"
        android:visibility="gone"/>


</androidx.cardview.widget.CardView>