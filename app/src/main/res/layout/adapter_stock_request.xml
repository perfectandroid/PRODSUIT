<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="0dp"
    android:backgroundTint="#E7EDED"
    android:layout_marginTop="0dp"
    app:cardElevation="4dp"
    app:cardUseCompatPadding="true">
    <!--    app:cardUseCompatPadding="true"-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:padding="2dp"
        android:paddingBottom="0dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/llstockrequest"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:orientation="vertical">

            <!--            android:background="@drawable/round_border_trans"-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="#F1F6F6"
                    android:padding="5dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:tint="@color/colorPrimaryDark"
                        android:src="@drawable/vtr_date"/>

                    <TextView
                        android:id="@+id/tv_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textSize="16dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="7dp"
                        android:textStyle="bold"
                        android:fontFamily="@font/myfont"
                        android:drawableTint="@color/colorPrimaryDark"
                        android:text="12-12-2023"
                        android:textColor="@color/colorPrimaryDark" />

                    <!--                    android:background="@drawable/round_border_trans1"-->

                </LinearLayout>






            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:drawableTint="@color/textColor1"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/myfont"
                        android:text="Branch"
                        android:textColor="@color/textColor1"
                        android:visibility="visible"/>



                    <TextView
                        android:id="@+id/tv_branch"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:layout_marginTop="-3dp"
                        android:textStyle="normal"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textColor="@color/textColor1" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:drawableTint="@color/textColor1"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/myfont"
                        android:text="Department From"
                        android:textColor="@color/textColor1"
                        android:visibility="visible"/>


                    <TextView
                        android:id="@+id/tv_department"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:layout_marginTop="-3dp"
                        android:textStyle="normal"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textColor="@color/textColor1" />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginTop="0dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:drawableTint="@color/textColor1"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/myfont"
                        android:text="Employee From"
                        android:textColor="@color/textColor1"
                        android:visibility="visible"/>


                    <TextView
                        android:id="@+id/tv_employee"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:layout_marginTop="-3dp"
                        android:textStyle="normal"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textColor="@color/textColor1" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginTop="0dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:drawableTint="@color/textColor1"
                        android:drawablePadding="5dp"
                        android:fontFamily="@font/myfont"
                        android:text="Employee To"
                        android:textColor="@color/textColor1"
                        android:visibility="visible"/>


                    <TextView
                        android:id="@+id/tv_employeeTo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="14dp"
                        android:layout_marginTop="-3dp"
                        android:textStyle="normal"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textColor="@color/textColor1" />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/tv_view"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/shape_bg_view"
                        android:fontFamily="@font/myfont"
                        android:gravity="center"
                        android:text="View"
                        android:textColor="#38795C"
                        android:textSize="16dp"
                        android:textStyle="normal"
                        android:visibility="visible" />

                    <!--                    E6E2E2-->


                </LinearLayout>

            </LinearLayout>


        </LinearLayout>


    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/list_line"
        android:visibility="gone"/>


</androidx.cardview.widget.CardView>
