<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="5dp"
    android:layout_marginLeft="2dp"
    android:layout_marginRight="2dp"
    android:backgroundTint="#FFFFFF"
    android:outlineAmbientShadowColor="@color/colorPrimaryDark"
    android:outlineSpotShadowColor="@color/colorPrimaryDark"
    app:cardElevation="2dp"
    android:layout_marginTop="0dp"
    app:cardUseCompatPadding="true">
    <!--    app:cardUseCompatPadding="true"-->
    <!--    android:backgroundTint="@color/white"-->



    <LinearLayout
        android:id="@+id/llServiceHistList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <!--        android:background="@drawable/svg_list_1"-->
        <!--        android:background="@drawable/border_shadow"-->

        <!--        android:background="@android:color/transparent"-->



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="5dp"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">



                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <!--<LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.8"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/svg_invoce_no"
                                app:tint="@color/colorPrimary" />
                            &lt;!&ndash;                            app:tint="@color/list_icon"&ndash;&gt;

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                android:fontFamily="@font/myfont"
                                android:text="Customer Due"
                                android:textColor="@color/list_label"
                                android:layout_centerVertical="true" />
                        </LinearLayout>
-->
                        <TextView
                            android:layout_width="5dp"
                            android:layout_height="match_parent"
                            android:gravity="start"
                            android:textSize="12dp"
                            android:fontFamily="@font/myfont"
                            android:text=""
                            android:textColor="@color/black"
                            android:layout_centerVertical="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">



                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_tktno"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/svg_invoce_date"
                                app:tint="@color/colorPrimary" />
                            <!--                            app:tint="@color/list_icon"-->

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:fontFamily="@font/myfont"
                                android:text="Ticket No"
                                android:textColor="@color/list_label"
                                android:layout_centerVertical="true" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="5dp"
                            android:layout_height="match_parent"
                            android:gravity="start"
                            android:textSize="12dp"
                            android:fontFamily="@font/myfont"
                            android:text=":"
                            android:textColor="@color/black"
                            android:layout_centerVertical="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txtv_ticketno"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:paddingLeft="2dp"
                                android:fontFamily="@font/myfont"
                                android:text=""
                                android:textStyle="normal"
                                android:textColor="@color/black"
                                android:layout_centerVertical="true" />

                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="1dp"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>




            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ll_customer"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/icon_svg_branch"
                                app:tint="@color/colorPrimary" />
                            <!--                            app:tint="@color/list_icon"-->

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:fontFamily="@font/myfont"
                                android:text="Customer"
                                android:textColor="@color/list_label"
                                android:layout_centerVertical="true" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="5dp"
                            android:layout_height="match_parent"
                            android:gravity="start"
                            android:textSize="12dp"
                            android:fontFamily="@font/myfont"
                            android:text=":"
                            android:textColor="@color/black"
                            android:layout_centerVertical="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txtv_customer"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:paddingLeft="2dp"
                                android:fontFamily="@font/myfont"
                                android:text=""
                                android:textStyle="normal"
                                android:textColor="@color/black"
                                android:layout_centerVertical="true" />

                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/ll_complnt"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/icon_svg_customer"
                                app:tint="@color/colorPrimary" />
                            <!--                            app:tint="@color/list_icon"-->

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:fontFamily="@font/myfont"
                                android:text="Address"
                                android:textColor="@color/list_label"
                                android:layout_centerVertical="true" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="5dp"
                            android:layout_height="match_parent"
                            android:gravity="start"
                            android:textSize="12dp"
                            android:fontFamily="@font/myfont"
                            android:text=":"
                            android:textColor="@color/black"
                            android:layout_centerVertical="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txtv_Address"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:paddingLeft="2dp"
                                android:fontFamily="@font/myfont"
                                android:text=""
                                android:textStyle="normal"
                                android:textColor="@color/black"
                                android:layout_centerVertical="true" />

                        </LinearLayout>



                    </LinearLayout>



                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="1dp"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:id="@+id/ll_date"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.8"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:padding="2dp"
                                android:src="@drawable/ic_date"
                                app:tint="@color/colorPrimary" />
                            <!--                            app:tint="@color/list_icon"-->

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:fontFamily="@font/myfont"
                                android:text="Date"
                                android:textColor="@color/list_label"
                                android:layout_centerVertical="true" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="5dp"
                            android:layout_height="match_parent"
                            android:gravity="start"
                            android:textSize="12dp"
                            android:fontFamily="@font/myfont"
                            android:text=":"
                            android:textColor="@color/black"
                            android:layout_centerVertical="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txt_date"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:paddingLeft="2dp"
                                android:fontFamily="@font/myfont"
                                android:text=""
                                android:textStyle="normal"
                                android:textColor="@color/black"
                                android:layout_centerVertical="true" />

                        </LinearLayout>



                    </LinearLayout>



                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="1dp"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">



                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">



                    <View
                        android:layout_width="0.5dp"
                        android:layout_height="1dp"
                        android:background="@color/color_background1"
                        android:visibility="gone"/>


                </LinearLayout>


            </LinearLayout>



            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:background="@color/grey"/>
            <LinearLayout
                android:gravity="right|center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                >
                <ImageView
                    android:id="@+id/img_share"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="13dp"
                    android:fontFamily="@font/myfont"
                    android:gravity="center"
                    android:padding="3dp"
                    android:src="@drawable/share_one"
                    app:tint="@color/colorPrimary" />

            </LinearLayout>




        </LinearLayout>

<!-- -->









    </LinearLayout>




</androidx.cardview.widget.CardView>