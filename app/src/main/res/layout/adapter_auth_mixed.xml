<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/white"
        app:cardCornerRadius="5dp"
        app:cardUseCompatPadding="true">

        <LinearLayout
            android:id="@+id/ll_leads"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#EBF3F3"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="5dp"
                android:background="#829393">

                <TextView
                    android:id="@+id/tv_heading"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/myfont"
                    android:text="Lead Generation"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    android:tint="@color/white" />

           </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="4dp"
                android:orientation="vertical">



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:padding="1dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:fontFamily="@font/myfont"
                        android:text="Transaction No"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/myfont"
                        android:text=":"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:tint="@color/black" />

                    <TextView
                        android:id="@+id/tv_TransactionNo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:padding="1dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:fontFamily="@font/myfont"
                        android:text="Date"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/myfont"
                        android:text=":"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:tint="@color/black" />

                    <TextView
                        android:id="@+id/tv_Date"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:padding="1dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:fontFamily="@font/myfont"
                        android:text="Entered By"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/myfont"
                        android:text=":"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:tint="@color/black" />

                    <TextView
                        android:id="@+id/tv_EnteredBy"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:padding="1dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:fontFamily="@font/myfont"
                        android:text="Entered On"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                    <TextView
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/myfont"
                        android:text=":"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:tint="@color/black" />

                    <TextView
                        android:id="@+id/tv_EnteredOn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="@font/myfont"
                        android:text=""
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="normal"
                        android:tint="@color/black" />

                </LinearLayout>


            </LinearLayout>






        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>