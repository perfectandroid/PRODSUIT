<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="0dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_header">

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imAdpBack"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_gravity="left"
                    android:background="@color/color_activity_transparent"
                    android:padding="10dp"
                    app:tint="@color/white"
                    android:visibility="invisible"
                    android:src="@drawable/back_arro"
                    tools:ignore="UseAppTint" />

            </LinearLayout>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Info"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:tint="@color/white"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:textStyle="bold"
                android:textAllCaps="false"
                android:fontFamily="@font/myfont" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imDetails"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="10dp"
                    android:background="@color/color_activity_transparent"
                    android:padding="10dp"
                    android:tint="@color/white"
                    android:src="@drawable/svg_service_details"
                    android:visibility="gone"
                    tools:ignore="UseAppTint" />
                <!--                    android:src="@drawable/svg_menu_dot"-->

            </LinearLayout>
        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    app:cardBackgroundColor="@color/color_fab1"
                    app:cardCornerRadius="3dp"
                    app:cardElevation="2dp"
                    app:cardMaxElevation="1dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:background="@color/color_fab1"
                            android:drawablePadding="10dp"
                            android:drawableTint="@color/textColor"
                            android:fontFamily="@font/myfont"
                            android:paddingLeft="10dp"
                            android:paddingTop="4dp"
                            android:paddingBottom="4dp"
                            android:text="Ticket Details"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/lin_ticketdetails"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:orientation="vertical"
                            android:paddingBottom="10dp"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:padding="2dp"
                                    android:src="@drawable/ic_ticket_details"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Ticket Number"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="14dp" />

                                    <TextView
                                        android:id="@+id/tv_ticket"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textSize="14dp" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:weightSum="2">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="horizontal"
                                    android:paddingLeft="10dp">

                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:layout_marginRight="5dp"
                                        android:padding="2dp"
                                        android:src="@drawable/ic_date"
                                        app:tint="@color/colorPrimary" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/myfont"
                                            android:singleLine="true"
                                            android:text="Registerd On"
                                            android:textColor="@color/colorPrimary"
                                            android:textSize="14dp" />

                                        <TextView
                                            android:id="@+id/tv_prod_regOn"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="-6dp"
                                            android:drawableTint="@color/greydark"
                                            android:fontFamily="@font/myfont"
                                            android:singleLine="true"
                                            android:textColor="@color/black"
                                            android:textSize="14dp" />
                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center">

                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:layout_gravity="center"
                                        android:layout_marginRight="5dp"
                                        android:padding="2dp"
                                        android:src="@drawable/ic_date"
                                        app:tint="@color/colorPrimary" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="@font/myfont"
                                            android:gravity=""
                                            android:singleLine="true"
                                            android:text="Visit On"
                                            android:textColor="@color/colorPrimary"
                                            android:textSize="14dp" />

                                        <TextView
                                            android:id="@+id/tv_visit_on"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="-6dp"
                                            android:drawableTint="@color/greydark"
                                            android:fontFamily="@font/myfont"
                                            android:singleLine="true"
                                            android:textColor="@color/black"
                                            android:textSize="14dp" />
                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>


                        </LinearLayout>


                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:visibility="visible"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="3dp"
                    app:cardElevation="2dp"
                    app:cardMaxElevation="1dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_CustClick"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/color_fab1"
                            android:drawablePadding="10dp"
                            android:drawableTint="@color/white"
                            android:fontFamily="@font/myfont"
                            android:paddingLeft="10dp"
                            android:paddingTop="4dp"
                            android:paddingBottom="4dp"
                            android:text="Customer Details"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/lin_CustClick"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:orientation="vertical"
                            android:paddingBottom="10dp"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:padding="3dp"
                                    android:src="@drawable/user_icon"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Customer Name"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_CustomerName"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textSize="13dp" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:padding="3dp"
                                    android:src="@drawable/phone_call"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Mobile"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_phone"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="9546662333"
                                        android:textColor="@color/black"
                                        android:textSize="13dp" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp"
                                android:visibility="gone">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/location"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Location"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_location"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textSize="13dp"
                                        android:textStyle="bold" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:padding="3dp"
                                    android:src="@drawable/address_svglist"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Address"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_address"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:textColor="@color/black"
                                        android:textSize="13dp" />
                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="5dp"
                    android:visibility="visible"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="3dp"
                    app:cardElevation="2dp"
                    app:cardMaxElevation="1dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_ProductDetails"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:drawablePadding="10dp"
                            android:background="@color/color_fab1"
                            android:drawableTint="@color/textColor"
                            android:fontFamily="@font/myfont"
                            android:paddingLeft="10dp"
                            android:paddingTop="4dp"
                            android:paddingBottom="4dp"
                            android:text="Product Details"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/lin_ProductDetails"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingBottom="10dp"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/lead_name"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="5dp"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Name"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_product_name"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textSize="13dp" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:padding="3dp"
                                    android:src="@drawable/category_icon"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Category  "
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_category"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textSize="13dp" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_gravity="center"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/service"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Service"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <TextView
                                        android:id="@+id/tv_service"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="-6dp"
                                        android:drawableTint="@color/greydark"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textSize="13dp" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingLeft="10dp">

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_marginRight="5dp"
                                    android:layout_marginTop="8dp"
                                    android:padding="2dp"
                                    android:src="@drawable/svg_notepad"
                                    app:tint="@color/colorPrimary" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginRight="10dp"
                                        android:fontFamily="@font/myfont"
                                        android:singleLine="true"
                                        android:text="Description"
                                        android:textColor="@color/colorPrimary"
                                        android:textSize="12dp" />

                                    <!--                            <ScrollView-->
                                    <!--                                android:layout_width="match_parent"-->
                                    <!--                                android:layout_height="wrap_content">-->
<!--                                    <androidx.core.widget.NestedScrollView-->
<!--                                        android:layout_width="match_parent"-->
<!--                                        android:layout_height="50dp">-->

                                        <TextView
                                            android:id="@+id/tv_description"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginTop="-6dp"
                                            android:drawableTint="@color/greydark"
                                            android:fontFamily="@font/myfont"
                                            android:scrollbars="vertical"
                                            android:text=""
                                            android:textColor="@color/black"
                                            android:textSize="13dp" />
<!--                                    </androidx.core.widget.NestedScrollView>-->
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>


                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>


        </ScrollView>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin_bottom_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:padding="10dp"
        android:visibility="visible"
        android:weightSum="2">


        <TextView
            android:id="@+id/tv_cancel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/btn_shape_submit"
            android:fontFamily="@font/myfont"
            android:gravity="center"
            android:padding="2dp"
            android:text="Close"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="14dp"
            android:textStyle="bold"
            android:visibility="visible" />


    </LinearLayout>
