                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="0dp"
                            app:cardElevation="0dp"
                            android:padding="1dp"
                            android:layout_marginTop="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp">

                            <LinearLayout
                                android:id="@+id/ll_pickup"
                                android:layout_width="match_parent"
                                android:layout_height="80dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:padding="1dp"
                                android:background="@drawable/border_1"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="100dp"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/shapes_1"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="80dp"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:orientation="vertical">
                                        <LinearLayout
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:background="@drawable/background_shape"
                                            android:gravity="center"
                                            android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="30dp"
                                            android:layout_height="30dp"
                                            android:src="@drawable/svg_pickup_truck"
                                            tools:ignore="UseAppTint" />

                                        </LinearLayout>
                                    </LinearLayout>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="20dp"
                                        android:text="PICK UP"
                                        android:layout_weight="1"
                                        android:textStyle="bold"
                                        android:textColor="@color/color_common1"
                                        android:textSize="20dp"
                                        android:fontFamily="@font/myfont" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="3"
                                        android:gravity="center"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/pick_up_count"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:text=""
                                            android:gravity="center"
                                            android:background="@drawable/counts_color1"
                                            android:textColor="@color/white"
                                            android:textSize="20dp"
                                            android:fontFamily="@font/myfont" />
                                    </LinearLayout>


                                </LinearLayout>





                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="20dp"/>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="0dp"
                            app:cardElevation="0dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp">

                            <!--                            android:backgroundTint="@color/service2"-->

                            <LinearLayout
                                android:id="@+id/ll_delivery"
                                android:layout_width="match_parent"
                                android:layout_height="80dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:padding="1dp"
                                android:background="@drawable/border_3"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="100dp"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/shapes_3"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="80dp"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:orientation="vertical">
                                        <LinearLayout
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:background="@drawable/background_shape"
                                            android:gravity="center"
                                            android:orientation="vertical">

                                        <ImageView
                                            android:layout_width="30dp"
                                            android:layout_height="30dp"
                                            android:src="@drawable/svg_delivery_bike"
                                            tools:ignore="UseAppTint" />
                                        </LinearLayout>

                                    </LinearLayout>




                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal">
                                    <!--                                    android:background="@drawable/svg_service_assign"-->


                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="20dp"
                                        android:text="DELIVERY"
                                        android:layout_weight="1"
                                        android:textStyle="bold"
                                        android:textColor="@color/color_common3"
                                        android:textSize="20dp"
                                        android:fontFamily="@font/myfont" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="3"
                                        android:gravity="center"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/delivery_count"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:text=""
                                            android:gravity="center"
                                            android:background="@drawable/count_color3"
                                            android:textColor="@color/white"
                                            android:textSize="20dp"
                                            android:fontFamily="@font/myfont" />
                                    </LinearLayout>


                                </LinearLayout>



                            </LinearLayout>

                        </androidx.cardview.widget.CardView>


                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="15dp"
                            app:cardElevation="5dp"
                            android:layout_marginTop="15dp"
                            android:layout_marginLeft="25dp"
                            android:layout_marginRight="25dp"
                            android:visibility="gone">

                            <LinearLayout
                                android:id="@+id/llCustomerService1"
                                android:layout_width="match_parent"
                                android:layout_height="80dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:background="@drawable/svg_cs_main"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="4"
                                    android:padding="15dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:src="@drawable/svg_icon_service_customer"
                                        android:tint="@color/white"
                                        tools:ignore="UseAppTint" />


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:background="@drawable/svg_cs_sub"
                                    android:layout_marginBottom="5dp"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:gravity="left"
                                        android:layout_marginTop="10dp"
                                        android:layout_marginBottom="10dp"
                                        android:text="Customer Service"
                                        android:textStyle="bold"
                                        android:textColor="@color/service11"
                                        android:textSize="20dp"
                                        android:fontFamily="@font/myfont" />


                                </LinearLayout>





                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

