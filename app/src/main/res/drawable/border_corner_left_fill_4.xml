<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="@color/color_common4"
                android:centerColor="@color/color_common4"
                android:startColor="@color/color_common4" />
            <corners android:bottomRightRadius="0dp" android:bottomLeftRadius="50dp"
                android:topLeftRadius="50dp" android:topRightRadius="0dp"/>
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_common4" />
            <corners android:bottomRightRadius="0dp" android:bottomLeftRadius="50dp"
                android:topLeftRadius="50dp" android:topRightRadius="0dp"/>
        </shape>
    </item>
</layer-list>
