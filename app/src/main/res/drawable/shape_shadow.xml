<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#c5d1b8" />
    <corners
        android:bottomLeftRadius="1dp"
        android:bottomRightRadius="0dp"
        android:topLeftRadius="1dp"
        android:topRightRadius="1dp" />
    <padding
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp" />
    <gradient
        android:angle="90"
        android:endColor="#DCEAEB"
        android:centerColor="#EAF3F3"
        android:startColor="#EAF2F3" />
    <stroke
        android:width="1dp"
        android:color="#D1E5E6" />
</shape>


